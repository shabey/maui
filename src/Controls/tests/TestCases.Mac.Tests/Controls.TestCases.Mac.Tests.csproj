<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFramework>$(_MauiDotNetTfm)</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsTestProject>true</IsTestProject>
    <RootNamespace>Microsoft.Maui.TestCases.Tests</RootNamespace>
    <DefineConstants>$(DefineConstants);MACCATALYST;MACUITEST;TEST_FAILS_ON_ANDROID;TEST_FAILS_ON_WINDOWS;TEST_FAILS_ON_IOS</DefineConstants>
  </PropertyGroup>
  
  <Import Project="..\AppiumTests.targets" />
  
  <ItemGroup>
    <Compile Include="..\TestCases.Shared.Tests\**\*.cs" Exclude="..\TestCases.Shared.Tests\obj\**;..\TestCases.Shared.Tests\bin\**" LinkBase="Shared" Visible="false" />
  </ItemGroup>

</Project>